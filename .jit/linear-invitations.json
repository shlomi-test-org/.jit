    "paths": {
        "/api/v1/rest/invitations": {
            "post": {
                "description": "Create an invitation",
                "operationId": "create_invitation",
                "parameters": [
                    {
                        "name": "invitation_type",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "invite_admin",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "boolean"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/CreateDeploymentRequestModel"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful Response"
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "CreateDeploymentRequestModel": {
                "type": "object",
                "properties": {
                    "repositoryId": {
                        "type": "string",
                        "description": "Unique identifier for the repository"
                    },
                    "deploymentName": {
                        "type": "string",
                        "description": "Name of the deployment"
                    },
                    "email": {
                        "type": "string",
                        "description": "Name of the deployment"
                    }
                },
                "required": ["repositoryId", "deploymentName"]
            }
        },
        "securitySchemes": {
            "APIKeyHeader": {
                "type": "apiKey",
                "in": "header",
                "name": "x-api-key"
            }
        }
    },
